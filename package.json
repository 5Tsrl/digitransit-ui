{
  "name": "openjourneyplanner-ui",
  "version": "0.0.1",
  "description": "Open Journey Planner UI",
  "author": "crew@reittiopas.fi",
  "scripts": {
    "folder": "mkdir -p _static/js; mkdir -p _static/css; mkdir -p _static/font;",
    "prestatic": "npm run folder",
    "static": "cp -r static/* _static; cp -r font/* _static/font/",
    "predev": "npm run folder",
    "dev": "NODE_ENV=development nodemon -e js,jsx,cjsx,css,scss,html --watch . --ignore ./_static/ server/server.js & NODE_ENV=development node ./hotLoadServer.js",
    "dev-nowatch": "NODE_ENV=development node server/server.js & NODE_ENV=development node ./hotLoadServer.js",
    "build": "NODE_ENV=production webpack -p --progress --colors",
    "start": "NODE_ENV=production nodemon server/server"
  },
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "dependencies": {
    "body-parser": "^1.12.3",
    "classnames": "^2.1.1",
    "cookie-parser": "^1.3.4",
    "es6-promise": "^2.1.1",
    "express": "^4.12.3",
    "fluxible": "^0.4.6",
    "fluxible-action-utils": "^0.2.1",
    "foundation-apps": "^1.1.0",
    "hogan-express": "^0.5.2",
    "httpify": "^1.0.0",
    "lodash": "^3.7.0",
    "masonry": "geta6/masonry",
    "moment": "^2.10.2",
    "mqtt": "^1.1.5",
    "node-cjsx": "^1.0.0",
    "polyline-encoded": "0.0.7",
    "react": "^0.13.2",
    "react-autosuggest": "^1.12.1",
    "react-leaflet": "^0.6.0",
    "react-router": "^0.13.2",
    "react-simpletabs": "^0.6.1",
    "serialize-javascript": "^1.0.0",
    "zurb-foundation-5": "^5.4.7"
  },
  "devDependencies": {
    "autoprefixer-core": "^5.1.11",
    "chance": "^0.7.5",
    "cjsx-loader": "^2.0.1",
    "coffee-loader": "^0.7.2",
    "css-loader": "^0.12.0",
    "csswring": "^3.0.5",
    "extract-text-webpack-plugin": "^0.7.1",
    "file-loader": "^0.8.1",
    "imports-loader": "^0.6.3",
    "jsx-loader": "0.13.2",
    "node-sass": "^3.0.0",
    "nodemon": "^1.3.7",
